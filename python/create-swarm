docker swarm init
docker-machine create --driver virtualbox myvm1 # Manager
docker-machine create --driver virtualbox myvm2 # Worker
docker-machine ls
docker-machine ssh myvm1 "docker swarm init --advertise-addr <myvm1 ip>" # Create Manager
docker-machine ssh myvm2 "docker swarm join --token <token> <myvm1 ip>:2377" #Create worker